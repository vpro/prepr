<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
  <Appenders>
    <Console name="stdout" target="SYSTEM_OUT">
      <PatternLayout pattern="%4.4p   -   %m  [ %c - %t ] %n"  charset="UTF-8"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="INFO">
      <Appender-Ref ref="stdout" />
    </Root>
  </Loggers>
</Configuration>
